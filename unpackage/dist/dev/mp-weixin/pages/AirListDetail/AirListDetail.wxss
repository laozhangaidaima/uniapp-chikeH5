page {
  background: #F2F6FC;
}
page .card {
  background: #ffffff;
  width: 94%;
  padding: 0 31.25rpx;
  margin: 20.833rpx auto;
  position: relative;
  border-radius: 6.25rpx;
}
page .card::before,
page .card::after {
  content: "";
  display: block;
  width: 41.667rpx;
  height: 41.667rpx;
  border-radius: 50%;
  position: absolute;
  top: 90.167rpx;
  z-index: 10000;
}
page .card::before {
  left: -20.833rpx;
  background-color: #02a6ff;
}
page .card::after {
  right: -20.833rpx;
  background-color: #3e5ceb;
}
page .productTypeName {
  width: 133.333rpx;
  height: 45.833rpx;
  line-height: 45.833rpx;
  font-size: 25rpx;
  border-top-left-radius: 10.417rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  color: #fff;
  text-align: center;
  margin-left: -4.167rpx;
}
page .bookBtn {
  width: 91.667rpx;
  height: 56.25rpx;
  line-height: 56.25rpx;
  font-size: 25rpx;
  color: #ffffff;
  background-color: #ff7625;
  border-radius: 8.333rpx;
  text-align: center;
}
page .detilMsg {
  line-height: 54.167rpx;
  width: 94%;
  margin: 20.833rpx auto;
  background: #fff;
  border-radius: 6.25rpx;
}

